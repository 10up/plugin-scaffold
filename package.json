{
  "name": "plugin-scaffold",
  "version": "1.0.0",
  "description": "10up Plugin Scaffold",
  "author": {
    "name": "10up",
    "email": "info@10up.com",
    "url": "https://10up.com",
    "role": "developer"
  },
  "scripts": {
    "test": "phpunit",
    "start": "npm install && composer install && gulp",
    "watch": "gulp watch",
    "build": "gulp",
    "deploy": "npm install && composer install --no-dev -o && gulp",
    "deploy-lint": "npm install && composer install && npm run lint",
    "lint-js": "eslint assets/js",
    "lint-php": "composer run lint",
    "format-js": "eslint --fix assets/js",
    "lint": "npm run lint-js && npm run lint-php",
    "format": "npm run format-js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/10up/plugin-scaffold"
  },
  "devDependencies": {
    "@10up/eslint-config": "^1.0.4",
    "@10up/stylelint-config": "^1.0.1",
    "babel-core": "^6.26.3",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.3",
    "babel-preset-env": "^1.7.0",
    "browser-sync": "^2.24.7",
    "browserslist": "^3.2.8",
    "caniuse-db": "^1.0.30000855",
    "del": "^3.0.0",
    "eslint": "^4.19.1",
    "eslint-loader": "^2.0.0",
    "gulp": "^4.0.0",
    "gulp-cssnano": "^2.1.3",
    "gulp-filter": "^5.1.0",
    "gulp-postcss": "^7.0.1",
    "gulp-rename": "^1.3.0",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-stylelint": "^7.0.0",
    "husky": "^1.0.0-rc.7",
    "lint-staged": "^7.1.2",
    "postcss-import": "^10.0.0",
    "postcss-preset-env": "^5.1.0",
    "pump": "^1.0.3",
    "require-dir": "^0.3.2",
    "stylelint": "^9.6.0",
    "stylelint-config-wordpress": "^13.1.0",
    "stylelint-declaration-use-variable": "^1.7.0",
    "stylelint-order": "^1.0.0",
    "webpack": "^4.12.0",
    "webpack-cli": "^2.1.5",
    "webpack-stream": "^3.2.0"
  },
  "engineStrict": true,
  "engines": {
    "node": "~8.11"
  },
  "dependencies": {
    "normalize.css": "^8.0.0"
  },
  "browserSync": {
    "proxyUrl": "http://tenup-scaffold.test"
  }
}
